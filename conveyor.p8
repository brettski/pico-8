function _init()
t=0
p_sprs={16,17,18}
p_sprc=1
p_spr=p_sprs[1]
end

function _update()
t+=1
if t%13==0 then
 p_sprc=t%3+1
 p_spr=p_sprs[p_sprc]
end
end

function _draw()
 cls()
 map() 
 print(p_spr,0,0,3)
 print(p_sprc,0,6,3)
 spr(p_spr,32,32)
 spr(p_spr+4,32,40)
end
