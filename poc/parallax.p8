pico-8 cartridge // http://www.pico-8.com
version 21
__lua__
--parallax
--brettski

function _init()
 map_x=0
 map_spd=5
 m1_x=0
 m1_spd=3
 m2_x=0
 m2_spd=1
end

function _update()
 --map_x-=map_spd
 --if map_x<-127 then
 -- map_x=0
 --end
 map_x=setx(map_x,map_spd)
 m1_x=setx(m1_x,m1_spd)
 m2_x=setx(m2_x,m2_spd)
end

function _draw()
 cls(12)
 map(32,0,m2_x,0,16,16)
 map(32,0,m2_x+128,0,16,16)
 map(16,0,m1_x,0,16,16)
 map(16,0,m1_x+128,0,16,16)
 map(0,0,map_x,0,16,16)
 map(0,0,map_x+128,0,16,16)
 spr(17,3*8,14*8)
end

function setx(_x,spd)
 _x-=spd
 if _x<-127 then
  return 0
 end
 return _x
end
