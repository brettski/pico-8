end

function _draw()
	cls(12)
	cursor(1,1)
	color(6)
	print("min by:"..bymin)
	print("max by:"..bymax)
	print("by :"..by)
	print("bdy:"..bdy)
	print("mapdx:"..mapdx)
 print("item:"..#items)
	
	map(0,0,mapx,mapy)
	spr(1,bx,by)
	
	for it in all(items) do
	 print(it.x..","..itemy)
	 spr(it.snum,it.x,itemy,2,1)
	end
	
end
