pico-8 cartridge // http://www.pico-8.com
version 22
__lua__

function _init()
 len=32 --r
 x0=64
 y0=64
 x1=x0+len
 y1=y0
 lines={}
 for i=185,270 do
  angle=i/360
  l={
   x=x0+sin(angle)*len,
   y=y0+cos(angle)*len,
  }
  add(lines,l)
 end
end

function _draw()
 cls(6)
 cursor(1,1,3)
 print("0   is 12 o'clock")
 print("270 is  3 o'clock")
 print("circle drawn clockwise")
 print("drawing 185 to 270")
 for ln in all(lines) do
  line(x0,y0,ln.x,ln.y,0)
 end
end
