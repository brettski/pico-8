function _init()
 buls={}
 for i=1, 1000 do
 	local bul={
 	 x=rnd(128),
 	 y=rnd(128),
 	}
 	add(buls,bul)
 end
end

function _update60()
 for bul in all(buls) do
  bul.y+=1
  if bul.y > 127 then
   bul.y=-5
   bul.x=rnd(128)
  end
 end
end

function _draw()
 cls()
 for bul in all(buls) do
  --spr(1, bul.x, bul.y)
  sspr(8,0,4,4,bul.x,bul.y)
  --circfill(bul.x,bul.y,4,2)
  --circfill(bul.x,bul.y,3,10)  
  --circfill(bul.x,bul.y,2,6)  
  --circfill(bul.x,bul.y,1,0)  
 end
end
