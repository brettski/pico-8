--gameplay

function moveplr(_x,_y)
 plr_x+=_x
 plr_y+=_y
end

--do flag tile
function doflag()
 local fp=field[getfkey(
                 plr_x-xoff,
                 plr_y-yoff)]

 local ste=fp.state
 if ste=="cvd" then
  fp.state="flg"
 elseif ste=="flg" then
  fp.state="cvd"
 end
end

--do select tile
function doselect()
 add(ckstack,
  getfkey(plr_x-xoff,
          plr_y-yoff))
 
 _upd=updfcover

end


