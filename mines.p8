function _init()
 t=0
 
 maxgamex=14
 maxgamey=14
 xfield=10   --field x max
 yfield=10   --field y max
 bombcount=10
 bombs={}    --bomb locations
 field={}    --playing field
 ckstack={}
 debug={}

 loaddata()
 loadanisets()
 gamestart()
 _upd=updategame
 _drw=drawgame
end

function _update60()
 t+=1
 
 _upd()
end

function _draw()

	_drw()
	drawdebug()
end


