--draws

function drawgame()
 cls(0)
 map()
 drawspr(plrani,plr_x*8,plr_y*8)
 drawspr(waterc,14*8,4*8,15)
 dodeskani()
end

function getframe(ani,spd)
 spd = spd or 10
 return ani[flr(t/spd)%#ani+1]
end

function dodeskani()
 for i=1,#deskani do
  drawspr(deskani[i],
   deskanix[i],
   deskaniy[i])
 end
end

function drawspr(_sprts,_x,_y,spd)
 palt(0,false)
 spr(getframe(_sprts,spd),_x,_y)
 pal()
end

function drawdebug()
 cursor(1,109)
 color(8)
 for txt in all(debug) do
  print(txt)
 end
end
