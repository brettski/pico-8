function _init()
 debug={}
 t=0
 initanidata()
 --left,right,up,down;0-3
 dirx={-1,1,0,0}
 diry={0,0,-1,1}
 
 _upd=updategame
 _drw=drawgame
 startgame()
end

function _update()
 t+=1
 _upd()
end

function _draw()
 _drw()
 drawdebug()
end

function startgame()
