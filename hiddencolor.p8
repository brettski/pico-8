--extended palette

function _init()
	t=0
	new=false
	--to always use ext palette
	--use:
	--poke(0xf52e,1)
	poke(0x5f2c,3) --64x64 screen
end
	
function _update()
	t+=1
end

function _draw()
	cls()

	if t%60 == 0 then
	 new=not new
	end
	if new then
  for c=0,15 do
   pal(c,c+128,1)
  end
 else
  pal()
 end
	map()

	
end

